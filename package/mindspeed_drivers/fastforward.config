config wifi_fastforward
    option ifname wlan1
    option direct_path_rx 1
    option direct_path_tx 1
config fastforward ftp
	option proto		tcp
	option port		21
config fastforward sip
	option port		5060
config fastforward rtsp
	option port		554
config fastforward pptp
	option port		1723
config fastforward h323
	option port		1720

config logging
	option file	-
	option command	0
	option error	1
	option warning	0
	option info	0
