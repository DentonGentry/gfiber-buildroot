<?xml version="1.0"?>
<cnfg>
    <MIDWARE_CONFIG>

    <!-- Decide whether midware should save its DB to flash at in interval  -->
    <saveDBflag   enabled="false" />
    <!--  The DB saving interval in unit of second, must >= 10, usually 10.  -->
    <saveDBInterval intervalTime="10" />
    
    </MIDWARE_CONFIG>
</cnfg>