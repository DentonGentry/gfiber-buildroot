# Don't Fast Forward FTP traffic
config fastforward ftp
	option proto		tcp
	option port		21
config fastforward sip
	option proto		udp
	option port		5060
config fastforward rtsp
	option port		554
config fastforward pptp
	option proto		tcp
	option port		1723
config fastforward h323
	option proto		tcp
	option port		1720

config logging
	option file	-
	option command	0
	option error	1
	option warning	0
	option info	0
