#!/bin/sh
#
# Returns true if this device's platform is operating as
# a network box.
#
. /etc/utils.sh

read plat </etc/platform

# FIXME(kedong): Since we use optimus for spacecast now, treat it as the
# network box until we remove the Qualcom switch on the real board.
startswith "$plat", "GFRG" || startswith "$plat", "GFSC" || startswith "$plat", "GFMN"
